    <div class="navbar-header">
        <a class="navbar-brand" href="/">
            <img id="company-logo" src="images/logo.png">
        </a>
    </div>
    <div class="navbar-right" id="nav-menu">
        <ul class="nav navbar-nav navbar-select">
            <li>
                <a href="#overview">Overview</a>
            </li>
            <li>
                <a href="#testimonial_blk">Testimonial</a>
            </li>

            <li>
                <a href="#pricing">Pricing</a>
            </li>

            <li>
                <a href="#support">Contact Us</a>
            </li>
            <li>
                <p>
                    <a href="/login" class="btn custom-button" id="login-btn-navbar">Log In</a>
                </p>
            </li>
        </ul>
    </div>
</nav>
<!-- start of outer container -->
<div class="container-fluid landing-pic">

    <div class="row" id="first-section">
        <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
            <img id="intro-image" src="images/ipad.png">
        </div>

        <div id="intro" class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
            <h1 class="heading bold text-center">The old rules of waiting are over.</h1>
            <p class="text-center info-text"> Let us help you revolutionize your patient waiting experience with
                effortless check-in, form management, arrival notification, and appointment integration for your
                office.</p>
            <form class="form-group" action="" method="post">
                <div class="col-xs-9">
                    <input type="company" class="small form-control" id="companyName" name="companyName"
                           value="{{companyName}}" method="post" placeholder="Step 1: Enter company name">
                </div>
                <div class="col-xs-1 text-small">
                    <button type="submit" class="btn custom-button">Sign Up</button>
                </div>
            </form>
        </div>
    </div>

    <div class="row">
        <section id="overview" style="padding: 50px">
            <div class="container">
                <div class="row" id="some-section">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <h1>Overview</h1>
                        </div>
                    </div>

                    <div class="row" style="padding: 20px">
                        <div class="col-sm-3 text-center">
                            <h4>Sign Up For An Account</h4>
                            <i class="fa fa-pencil-square-o fa-5x" style="color:#ffae00"></i>
                            <br><br>
                            <p class="overviewColor">
                                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nam volutpat purus in ferment
                                uectetur tortor id, pharetra lorem.
                            </p>
                        </div>
                        <div class="col-sm-3 text-center">
                            <h4>Customize Your Own Forms </h4>
                            <i class="fa fa-file-text-o fa-5x" style="color:#ffae00"></i>
                            <br><br>
                            <p class="overviewColor">
                                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nam volutpat purus in ferment
                                uectetur tortor id, pharetra lorem.
                            </p>
                        </div>
                        <div class="col-sm-3 text-center">
                            <h4>Deploy Onto Device </h4>
                            <i class="fa fa-tablet fa-5x" style="color:#ffae00"></i>
                            <br><br>
                            <p class="overviewColor">
                                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nam volutpat purus in ferment
                                uectetur tortor id, pharetra lorem.
                            </p>
                        </div>
                        <div class="col-sm-3 text-center">
                            <h4>Manage Your Customers </h4>
                            <i class="fa fa-users fa-5x" style="color: #ffae00;"></i>
                            <br><br>
                            <p class="overviewColor">
                                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nam volutpat purus in ferment
                                uectetur tortor id, pharetra lorem.
                            </p>
                        </div>

                    </div>
                </div>
            </div>

        </section>

        <section id="testimonial_blk" style="padding: 50px; background-color: #e6e6e6;">
            <div class="container">
                <div class="row" id="second-section">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <h1>Testimonials</h1>
                        </div>
                    </div>

                    <div class="col-xs-6">
                        <div>
                            <blockquote class="testimonial">
                                <p>With a limited budget and tight schedule, this check-in app really saved me time
                                    and cost to run the dental clinic I just opened up. Thank you, iReceptionist.
                                    You’re a life-saver.</p>
                            </blockquote>
                            <div class="arrow-down"></div>
                            <img src="images/jennifer.jpg" class="testimonial-image">
                            <span class="testimonial-author">Dr. Jennifer Kim, DDS | <span>San Francisco, California</span></span>
                        </div>
                        <div>
                            <blockquote class="testimonial testimonial-grey">
                                <p>Wow! iReceptionist was the perfect addition to our offices! It’s elegant, fast,
                                    and our customers absolutely love it! The team’s continued support and
                                    commitment to excellence has made me a life-long customer. Definitely the GOLD
                                    standard for check-in apps!</p>
                            </blockquote>
                            <div class="arrow-down arrow-down-grey"></div>
                            <img src="images/leonard.jpg" class="testimonial-image">
                            <span class="testimonial-author">Dr. Leonard McCoy, M.D. | <span>Riverside, California</span></span>

                        </div>
                    </div>
                    <div class="col-xs-6">
                        <div>
                            <blockquote class="testimonial testimonial-grey">
                                <p>iReceptionist is truly an aesthetic experience, I love how intuitive and modern
                                    the design is. Making appointments and checking in has never been easier.</p>
                            </blockquote>
                            <div class="arrow-down arrow-down-grey"></div>
                            <img src="images/betty.jpg" class="testimonial-image">
                            <span class="testimonial-author">Betty Johnson | <span>Stockholm, Sweden</span></span>
                        </div>
                        <div>
                            <blockquote class="testimonial">
                                <p>I can’t believe how much time I saved using iReceptionist. It was truly a breathe
                                    of fresh air. Never again will I have to deal with the hassle of a grumpy
                                    receptionist and a clipboard!</p>
                            </blockquote>
                            <div class="arrow-down"></div>
                            <img src="images/joel.jpg" class="testimonial-image">
                            <span class="testimonial-author">Joel Spolsky | <span>Kathmandu, Nepal</span></span>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <section id="pricing" style="padding: 50px">
            <div class="row" id="third-section">
                <!-- Pricing Header-->
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h1>Pricing</h1>
                        <br><br>
                    </div>
                </div>

                <!--Container for Pricing Chart-->
                <div class="row">
                    <!--Filler to center Pricing Chart-->
                    <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
                    </div>

                    <!--First Pricing Chart (FREE) -->
                    <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">

                        <div class="panel price panel-green">
                            <div class="panel-heading arrow_box text-center">
                                <h3>FREE</h3>
                            </div>

                            <!--Pricing-->
                            <div class="panel-body text-center">
                                <p class="lead" style="font-size:40px"><strong>$0 / month</strong></p>
                            </div>

                            <!-- Features -->
                            <ul class="list-group list-group-flush text-center">
                                <li class="list-group-item"><i class="fa fa-check" id="chartCheck"></i> Personal use
                                </li>
                                <li class="list-group-item"><i class="fa fa-check" id="chartCheck"></i>Unlimited
                                    projects
                                </li>
                                <li class="list-group-item"><i class="fa fa-times" id="chartX"></i>24/7 support</li>
                            </ul>

                            <!-- Button (Currently no function) -->
                            <div class="panel-footer">
                                <a class="btn btn-lg btn-block btn-success" href="#">SIGN UP!</a>
                            </div>
                        </div>
                    </div>

                    <!-- Second Pricing Chart (PREMIUM) -->
                    <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">


                        <div class="panel price panel-red">
                            <div class="panel-heading  text-center">
                                <h3>PREMIUM</h3>
                            </div>

                            <!--Cost-->
                            <div class="panel-body text-center">
                                <p class="lead" style="font-size:40px"><strong>$10 / month</strong></p>
                            </div>

                            <!-- Features -->
                            <ul class="list-group list-group-flush text-center">
                                <li class="list-group-item"><i class="fa fa-check" id="chartCheck"></i> Personal use
                                </li>
                                <li class="list-group-item"><i class="fa fa-check" id="chartCheck"></i> Unlimited
                                    projects
                                </li>
                                <li class="list-group-item"><i class="fa fa-check" id="chartCheck"></i> 24/7 support
                                </li>
                            </ul>

                            <!--Button to purchase (No Function)-->
                            <div class="panel-footer">
                                <a class="btn btn-lg btn-block btn-danger" href="#">BUY NOW!</a>
                            </div>
                        </div>

                    </div>

                    <!--Filler to center two pricing charts-->
                    <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
                    </div>

                </div>

            </div>
        </section>
