    <div class="row">
        <section id="support" style="padding: 50px; background-color: #e6e6e6; margin:0">
            <div class="row" id="fourth-section">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h1>Contact Us!</h1>
                        <h4>Email us at: fubar@fubar.co</h4>
                        <h4>Call us at: 605-475-6968 </h4>
                    </div>
                </div>
            </div>
        </section>
